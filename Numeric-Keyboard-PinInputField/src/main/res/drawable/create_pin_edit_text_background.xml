<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools" xmlns:app="http://schemas.android.com/apk/res-auto" android:color="@android:color/transparent" tools:targetApi="lollipop">
    <item
        app:state_error="false"
        app:state_active_field="false"
        android:drawable="@drawable/default_state"
        android:id="@android:id/background">
    </item>
    <item
        app:state_active_field="true"
        app:state_error="false"
        android:drawable="@drawable/active_state"
        android:id="@android:id/background">
    </item>
    <item
        app:state_error="true"
        app:state_active_field="false"
        android:drawable="@drawable/error_state"
        android:id="@android:id/background">
    </item>



</selector>